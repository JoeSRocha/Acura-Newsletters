include ./image.pug
mixin spacer(props)
	+image({
		"src": constants.ImagePath + "spacer.png",
		"height": props.height ? props.height : "1",
		"width": props.width ? props.width : "1",
		"style": {
			height : props.height ? props.height + "px;" : "1px",
			width: props.width ? props.width + "px;" : "1px",
			"background-color": props.color ? props.color : null
		}
	})